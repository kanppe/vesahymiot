    "css": ["jquery-ui.css"],
    "js": [ "jquery.min.js", "jquery-ui.min.js", "main.js" ],
    "matches": [ "http://*.vesabbs.fi/*", "https://*.vesabbs.fi/*", "http://*.vesabbs.com/*", "https://*.vesabbs.com/*"]
  }],
  "web_accessible_resources": ["jquery.min.map"]
}
